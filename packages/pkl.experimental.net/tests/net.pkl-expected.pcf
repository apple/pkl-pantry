examples {
  ["expandIPv6AddressString"] {
    "fe80:0000:0000:0000:0000:0000:0000:0000"
    "fe80:0000:0000:0000:0000:0000:0000:0001"
    "0000:0000:0000:0000:0000:0000:0000:00aa"
  }
  ["expandIPv6AddressString with scoped addresses"] {
    "fe80:0000:0000:0000:0000:0000:0000:0000%1"
    "fe80:0000:0000:0000:0000:0000:0000:0000%0"
    "fe80:0000:0000:0000:0005:0006:0007:0008%1"
    "fe80:0000:0000:0000:0000:0000:0000:0000%000"
    "fe80:0000:0000:0000:0000:0000:0000:0000%001"
  }
  ["expandIPv6AddressString with IPv4 suffix"] {
    "ffff:ffff:ffff:ffff:0000:0000:255.255.255.255"
    "0000:0000:0000:0000:0000:0000:1.2.3.4"
    "2001:0db8:0000:0000:0000:0000:1.2.3.4"
    "000a:000b:000c:000d:000e:000f:1.0.30.0"
  }
  ["compressIPv6AddressString"] {
    "1:2:3:4:5:6:7:8"
    "fe80::1:0:0:1"
    "fe80::1:0:0:1"
    "1:2:3:4:0:6:7:8"
    "1:2:3:4:0:6:7:8"
    "::"
    "::"
    "fe80:0:a:b:c:d:e:f"
    "2001:0:0:1::1"
    "2001:db8::1:0:0:1"
  }
  ["compressIPv6AddressString with scoped addresses"] {
    "fe80::5:6:7:8%0"
    "fe80::1:0%0"
    "fe80::%0"
    "fe80::%1"
    "fe80::%0"
    "fe80::%1"
  }
  ["compressIPv6AddressString with IPv4 suffix"] {
    "::ffff:1.2.3.4"
    "::ffff:1.20.0.4"
    "fe80::1.2.3.4"
    "fe80::0.0.0.0"
    "::0.0.0.0"
  }
}
