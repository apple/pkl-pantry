  ["Scoped IPv6AddressString (link-local unicast)"] {
    "fe80::%1" is net.IPv6AddressString
    "FE80::%1" is net.IPv6AddressString
    "fe80::%999" is net.IPv6AddressString
    "fe80:0:0:0:0:0:0:0%1" is net.IPv6AddressString
