examples {
  ["exampleGateway.pkl"] {
    """
    apiVersion: gateway.networking.k8s.io/v1
    kind: Gateway
    metadata:
      namespace: example-gateway
    spec:
      gatewayClassName: example-gateway-class
      listeners:
      - name: http
        port: 80
        protocol: HTTP
    
    """
  }
  ["exampleRoute.pkl"] {
    """
    apiVersion: gateway.networking.k8s.io/v1
    kind: HTTPRoute
    metadata:
      name: example-route
    spec:
      hostnames:
      - example.com
      parentRefs:
      - name: example-gateway
      rules:
      - backendRefs:
        - name: example-svc
          port: 80
    
    """
  }
}
