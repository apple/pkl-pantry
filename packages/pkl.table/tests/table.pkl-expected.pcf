examples {
  ["simple"] {
    """
    +--------------+------+-------+
    | alpha        | beta | gamma |
    +--------------+------+-------+
    | 123          | 123  | 123.h |
    | 456          | 456  | 456.h |
    | 789loooooong | 789  | 789.h |
    +--------------+------+-------+
    
    """
  }
  ["fancy column"] {
    """
    +--------------+-----+-------+---------+
    |            a | b   | c     | unknown |
    +--------------+-----+-------+---------+
    |          123 | 123 | 123.h | <null>  |
    |          456 | 456 | 456.h | <null>  |
    | 789loooooong | 789 | 789.h | <null>  |
    +--------------+-----+-------+---------+
    
    """
  }
  ["row directive"] {
    """
    <table>
      <thead>
        <tr>
          <th>alpha</th>
          <th>beta</th>
          <th>gamma</th>
        </tr>
      </thead>
      <tbody>
        <tr>
          <td>123</td>
          <td>123</td>
          <td>123.h</td>
        </tr>
        <tr>
          <td>456</td>
          <td>456</td>
          <td>456.h</td>
        </tr>
        <tr>
          <td>789loooooong</td>
          <td>789</td>
          <td>789.h</td>
        </tr>
        <tr><th colspan="3">this is a colspan</th></tr>
      </tbody>
    </table>
    
    """
  }
  ["coverter"] {
    """
    +--------------+------+-----------+-------+
    | alpha        | beta | gamma     | delta |
